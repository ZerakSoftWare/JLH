platform :ios,'8.0'
def myPods
    pod 'AFNetworking', '~> 3.1.0'
    pod 'Reachability', '~> 3.2'
    pod 'MJExtension', '~> 3.0.13'
    pod 'MJRefresh', '~> 3.1.12'
    pod 'Masonry', '~> 1.0.2'
    pod 'SDWebImage', '~> 4.0.0-beta2'
    pod 'FMDB', '~> 2.6.2'
    #键盘
    pod 'JCRouter', :git => 'https://github.com/SerilesJam/JCRouterDemo.git'
    pod 'IQKeyboardManager', '~> 5.0.0'
    pod 'CocoaLumberjack'
#    pod 'CocoaLumberjack', :git => 'git@github.com:supertop/CocoaLumberjack.git', :commit => '3443875e'

    # 友盟分享UI
    pod 'UMengUShare/UI'
    # 集成新浪微博
    pod 'UMengUShare/Social/Sina'
    # 集成微信
    pod 'UMengUShare/Social/WeChat'
    # 集成QQ
    pod 'UMengUShare/Social/QQ'
    
    #svga文件播放器
    pod 'SVGAPlayer'

    # 图片浏览
    pod 'MWPhotoBrowser', :git => 'https://github.com/EvoIos/MWPhotoBrowser.git', :commit => 'de697e101195557ddca85661ebb266fd3f10776c'
    
    pod 'UMengAnalytics' #标准SDK，含IDFA
    pod 'UMessage', '~> 1.4.0'
    pod 'FLEX', :configurations => ['Debug']
    pod 'ZJAnimationPopView'

end

target 'JieLeHua' do
    myPods
end

target 'JieLeHuaQuick' do
    myPods
end
